    "## RMSE Calculation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "38bf1861",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "1.0060697360748054"
      ]
     },
     "execution_count": 35,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "#calculate mean of random dataset\n",
    "\n",
    "import statistics\n",
    "z=statistics.mean(y)\n",
    "\n",
    "#finding square of data minus average\n",
    "\n",
    "a=(y-z)**2\n",
    "\n",
    "\n",
    "\n",
    "#final calculation\n",
    "\n",
    "RMSE=np.sqrt((1/10000)*sum(a))\n",
    "RMSE\n",
    "\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
