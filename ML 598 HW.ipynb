   ],
   "source": [
    "#verifying RMSE calculation\n",
    "import math\n",
    "y_actual = statistics.mean(y)\n",
    "y_predicted = y\n",
    " \n",
    "MSE = np.square(np.subtract(y_actual,y_predicted)).mean() \n",
    " \n",
    "RMSE = math.sqrt(MSE)\n",
    "RMSE"
